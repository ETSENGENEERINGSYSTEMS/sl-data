[ 1a-final date from event ]
if
  ( the academic document AD has no date D as final date ) and
  ( the academic document AD has the event E as venue ) and
  ( the event E corresponds to the date ED )
then
  ( the academic document AD has the date ED as final date )
.

[ 1b-final date from publish date ]
if
  ( the academic document AD has the date PD as publish date ) and
  ( the academic document AD has no date D as final date )
then
  ( the academic document AD has the date PD as final date )
.
